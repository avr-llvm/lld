# RUN: lld -core %s | FileCheck %s

#
# Test that section groups are parsed and the first group selected for symbol
# resolution
#

---
defined-atoms:
  - name:            f1
    scope:           global
    type:            code
  - name:            f2
    scope:           global
    type:            code
  - name:            g1
    scope:           global
    type:            code
  - name:            d1
    scope:           global
    type:            data
  - name:            g1
    scope:           global
    type:            group-comdat
    references:
      - kind:            group-child
        target:          f1
      - kind:            group-child
        target:          f2
      - kind:            group-child
        target:          g1
      - kind:            group-child
        target:          d1
---
defined-atoms:
  - name:            f1
    scope:           global
    type:            code
  - name:            f2
    scope:           global
    type:            code
  - name:            g1
    scope:           global
    type:            code
  - name:            d1
    scope:           global
    type:            data
  - name:            g1
    scope:           global
    type:            group-comdat
    references:
      - kind:            group-child
        target:          f1
      - kind:            group-child
        target:          f2
      - kind:            group-child
        target:          g1
      - kind:            group-child
        target:          d1
...

#CHECK: defined-atoms:
#CHECK:   - name:            g1
#CHECK:     ref-name:        [[PARENT:[a-zA-Z\.0-9_]+]]
#CHECK:     scope:           global
#CHECK:     type:            group-comdat
#CHECK:     references:
#CHECK:       - kind:            group-child
#CHECK:         offset:          0
#CHECK:         target:          f1
#CHECK:       - kind:            group-child
#CHECK:         offset:          0
#CHECK:         target:          f2
#CHECK:       - kind:            group-child
#CHECK:         offset:          0
#CHECK:         target:          [[GCHILD:[a-zA-Z\.0-9_]+]]
#CHECK:       - kind:            group-child
#CHECK:         offset:          0
#CHECK:         target:          d1
#CHECK:   - name:            f1
#CHECK:     scope:           global
#CHECK:   - name:            f2
#CHECK:     scope:           global
#CHECK:   - name:            g1
#CHECK:     ref-name:        [[GCHILD]]
#CHECK:     scope:           global
#CHECK:   - name:            d1
#CHECK:     scope:           global
#CHECK:     type:            data
#CHECK: ...
